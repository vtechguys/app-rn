{
  "expo": {
    "name": "thunderpod",
    "slug": "thunderpod",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bacon.expo.googlesignin",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.521553643049-c628a93bu2eidorq1aij20ctlco16kt2"
        }
      }
    },
    "android": {
      "package": "com.bacon.expo.googlesignin",
      "googleServicesFile": "./services/google-services.json"
    },
    "description": "v 0.0.1 only implements pedometer count steps",
    "githubUrl": "https://github.com/vtechguys/react-navigation-v3-issue"
  }
}